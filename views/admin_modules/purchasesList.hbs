<!-- Content Header (Page header) -->
<section class="content-header">
  <h1>
        LOGS
      </h1>
</section>
<!-- Main content -->
<section class="content">
  <div class="row">
    <!-- left column -->
    <div class="col-md-12">
      <!-- general form elements -->
      <div class="box box-primary">
        <div class="box-header with-border">
          <h3 class="box-title">Purchase LOG </h3>
        </div>
        <div class="box-body table-responsive no-padding">
          <table class="table table-hover">
            <tbody>
              <tr>
                <th>Date</th>
                <th>Account</th>
                <th>Mu Coins</th>
                <th>Comment</th>
              </tr>
              {{#each pLOG}}
                <tr>
                  <td>{{timestamp}}</td>
                  <td>{{account}}</td>
                  <td>
                    <p>{{mucoins}}</p>
                  </td>
                  <td>
                    <p>{{message}}</p>
                  </td>
                </tr>
              {{/each}}
            </tbody>
          </table>
        </div>
        <!-- /.box-header -->
        <!-- form start -->
      </div>
    </div>
    <!-- /.box-body -->
</section>
<!-- /.content -->
