<!DOCTYPE html>
<!--
This is a starter template page. Use this page to start your new project from
scratch. This page gets rid of all links and provides the needed markup only.
-->
<html>
<head>
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <title>{{pageName}} CP - {{moduleTitle}}</title>
  <!-- Tell the browser to be responsive to screen width -->
  <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
  <!-- Bootstrap 3.3.6 -->
  <link rel="stylesheet" href="/admincp/bootstrap/css/bootstrap.min.css">
  <!-- Ionicons -->
  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/ionicons/2.0.1/css/ionicons.min.css">
  <!-- Theme style -->
  <link rel="stylesheet" href="/admincp/rel/css/AdminLTE.min.css">
  <!-- AdminLTE Skins. We have chosen the skin-blue for this starter
        page. However, you can choose any other skin. Make sure you
        apply the skin class to the body tag so the changes take effect.
  -->
  <link rel="stylesheet" href="/admincp/plugins/select2/select2.min.css" />
  <link rel="stylesheet" href="/admincp/plugins/select2/select2-bootstrap.min.css" />
  
  <link rel="stylesheet" href="/admincp/rel/css/skins/skin-purple.min.css">

  <!--[if lt IE 9]>
  <script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
  <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
  <![endif]-->
</head>
<body class="hold-transition skin-purple sidebar-mini">
<div class="wrapper">

  <!-- Main Header -->
  <header class="main-header">

    <!-- Logo -->
    <a href="/" class="logo">
      <!-- mini logo for sidebar mini 50x50 pixels -->
      <span class="logo-mini"><b>MW</b></span>
      <!-- logo for regular state and mobile devices -->
      <span class="logo-lg"><b>MuWeb</b> Control Panel</span>
    </a>

    <!-- Header Navbar -->
    <nav class="navbar navbar-static-top" role="navigation">
      <!-- Sidebar toggle button-->
      <a href="#" class="sidebar-toggle" data-toggle="offcanvas" role="button">
        <span class="sr-only">Toggle navigation</span>
      </a>
      <!-- Navbar Right Menu -->
      <div class="navbar-custom-menu">
        <ul class="nav navbar-nav">
          <!-- User Account Menu -->
          <li class="dropdown user user-menu">
            <!-- Menu Toggle Button -->
            <a href="#" class="dropdown-toggle" data-toggle="dropdown">
              <!-- The user image in the navbar-->
              <i class="fa fa-user"></i>
              <!-- hidden-xs hides the username on small devices so only the image appears. -->
              <span class="hidden-xs">{{auth.memb___id}}</span>
            </a>
            <ul class="dropdown-menu">
                <li><a href="/"><i class="fa fa-desktop fa-fw" aria-hidden="true"></i> Ver WEB</a></li>
                <li role="separator" class="divider"></li>
                <li><a href="/auth/logout"><i class="fa fa-sign-out fa-fw" aria-hidden="true"></i> Salir</a></li>
            </ul>
          </li>
          <!-- Control Sidebar Toggle Button -->
        </ul>
      </div>
    </nav>
  </header>
  <!-- Left side column. contains the logo and sidebar -->
  <aside class="main-sidebar">

    <!-- sidebar: style can be found in sidebar.less -->
    <section class="sidebar">

      <!-- Sidebar Menu -->
      <ul class="sidebar-menu">
        <li class="header">Menu</li>
        <!-- Optionally, you can add icons to the links -->
        <li {{#if active.home}}class="active"{{/if}}><a href="/admincp/"><i class="fa fa-home"></i> <span>Home</span></a></li>
        <li {{#if active.general}}class="active"{{/if}}><a href="/admincp/general"><i class="fa fa-gear"></i> <span>General Settings</span></a></li>
        <li class="treeview {{#if active.news}}active{{/if}}">
          <a href="#"><i class="fa fa-newspaper-o"></i> <span>News</span>
            <span class="pull-right-container">
              <i class="fa fa-angle-left pull-right"></i>
            </span>
          </a>
          <ul class="treeview-menu">
            <li {{#if active.addNew}}class="active"{{/if}}><a href="/admincp/news/add">Add a new..</a></li>
            <li {{#if active.newsList}}class="active"{{/if}}><a href="/admincp/news">News list</a></li>
          </ul>
        </li>
        <li class="treeview {{#if active.userSettings}}active{{/if}}">
          <a href="#"><i class="fa fa-users"></i> <span>MODULES Settings</span>
            <span class="pull-right-container">
              <i class="fa fa-angle-left pull-right"></i>
            </span>
          </a>
          <ul class="treeview-menu">
            <li {{#if active.modules}}class="active"{{/if}}><a href="/admincp/modules"><span>Active Settings</span></a></li>
            <li {{#if active.reset}}class="active"{{/if}}><a href="/admincp/reset"><span>RESET Settings</span></a></li>
            <li {{#if active.pkclear}}class="active"{{/if}}><a href="/admincp/pkclear"><span>PKCLEAR Settings</span></a></li>
            <li {{#if active.unstuck}}class="active"{{/if}}><a href="/admincp/unstuck"><span>UNSTUCK Settings</span></a></li>
            <li {{#if active.addstats}}class="active"{{/if}}><a href="/admincp/addstats"><span>ADDSTATS Settings</span></a></li>
            <li {{#if active.vip}}class="active"{{/if}}><a href="/admincp/vip"><span>VIP Settings</span></a></li>
          </ul>
        </li>
        <li {{#if active.marketplaceSettings}}class="active"{{/if}}><a href="/admincp/marketplace"><i class="fa fa-shopping-bag"></i> <span>Marketplace Settings</span></a></li>
        <li class="treeview {{#if active.webshop}}active{{/if}}">
          <a href="#"><i class="fa fa-shopping-cart"></i> <span>Webshop</span>
            <span class="pull-right-container">
              <i class="fa fa-angle-left pull-right"></i>
            </span>
          </a>
          <ul class="treeview-menu">
            <li {{#if active.addCategory}}class="active"{{/if}}><a href="/admincp/webshop/category/add">Add a category</a></li>
            <li {{#if active.addItem}}class="active"{{/if}}><a href="/admincp/webshop/item/add">Add an item</a></li>
            <li {{#if active.webshopList}}class="active"{{/if}}><a href="/admincp/webshop">Listings</a></li>
            <li {{#if active.webshopSettings}}class="active"{{/if}}><a href="/admincp/webshop/settings">Settings</a></li>
          </ul>
        </li>
        <li class="treeview {{#if active.logs}}active{{/if}}">
          <a href="#"><i class="fa fa-list"></i> <span>Logs</span>
            <span class="pull-right-container">
              <i class="fa fa-angle-left pull-right"></i>
            </span>
          </a>
          <ul class="treeview-menu">
            <li {{#if active.error}}class="active"{{/if}}><a href="/admincp/logs/error">Error LOGS</a></li>
            <li {{#if active.purchase}}class="active"{{/if}}><a href="/admincp/logs/purchases">Purchases LOGS</a></li>
          </ul>
        </li>
      </ul>
      <!-- /.sidebar-menu -->
    </section>
    <!-- /.sidebar -->
  </aside>

  <!-- Content Wrapper. Contains page content -->
  <div class="content-wrapper">
    {{> (module)}}
  </div>
  <!-- /.content-wrapper -->

  <!-- Main Footer -->
  <footer class="main-footer">
    <!-- To the right -->
    <!-- Default to the left -->
    <strong>Copyright &copy; 2016 Emmanuel Vazquez.</strong> All rights reserved.
  </footer>

<!-- REQUIRED JS SCRIPTS -->

<!-- jQuery 2.2.3 -->
<script src="/admincp/plugins/jQuery/jquery-2.2.3.min.js"></script>
<!-- Bootstrap 3.3.6 -->
<script src="/admincp/bootstrap/js/bootstrap.min.js"></script>
<!-- AdminLTE App -->
<script src="/admincp/rel/js/app.min.js"></script>
<!-- Font Awesome -->
<script src="https://use.fontawesome.com/4d6d0433bf.js"></script>

<!-- Optionally, you can add Slimscroll and FastClick plugins.
     Both of these plugins are recommended to enhance the
     user experience. Slimscroll is required when using the
     fixed layout. -->
</body>
</html>
