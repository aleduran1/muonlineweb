{
  "name": "MuWeb",
  "version": "1.0.0",
  "private": false,
  "description": "Mu WEB for MuEmu servers by Maanu",
  "main": "src/main.js",
  "scripts": {
    "start": "node dist/main.js",
    "production": "pm2 start app.json --watch",
    "dev": "nodemon --config nodemon.json --exec babel-node src/main.js",
    "build-backend": "babel src -d dist",
    "build-frontend": "webpack --config webpack.config.js",
    "build-views": "node cli.js -build",
    "build": "npm run build-backend && npm run build-frontend && npm run build-views",
    "devfe": "webpack --progress --colors --config webpack.config.js --watch"
  },
  "keywords": [],
  "author": "Maanu",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.17.1",
    "body-parser": "^1.15.2",
    "bootstrap": "^3.3.7",
    "connect-redis": "^3.1.0",
    "cookie-parser": "^1.4.3",
    "countdown": "^2.6.0",
    "csurf": "^1.9.0",
    "express": "^4.14.0",
    "express-flash": "0.0.2",
    "express-session": "^1.14.1",
    "express-sitemap": "^1.6.5",
    "express-validator": "^3.2.0",
    "form-data": "^2.3.1",
    "fs-extra": "^3.0.1",
    "glob": "^7.1.2",
    "hbs": "^4.0.1",
    "imagesloaded": "^4.1.2",
    "jquery": "^3.2.1",
    "knex": "^0.13.0",
    "moment": "^2.15.1",
    "moment-countdown": "0.0.3",
    "money": "^0.2.0",
    "morgan": "^1.7.0",
    "mssql": "^3.3.0",
    "nodemailer": "^4.0.1",
    "nodemailer-express-handlebars": "^2.0.0",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "pm2": "^2.1.6",
    "promise-polyfill": "^6.0.2",
    "serve-favicon": "^2.3.2",
    "whatwg-fetch": "^2.0.3",
    "winston": "^2.3.1"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
