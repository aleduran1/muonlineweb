{
    "apps": [
        {
            "name": "MuWeb",
            "script": "./dist/main.js",
            "watch": [
                "config.json",
                "./views/**/*.hbs"
            ],
            "instances": 0,
            "max_memory_restart": "2G",
            "exec_mode": "cluster",
            "error_file": "./logs/error.log",
            "out_file": "./logs/out.log",
            "merge_logs": true,
            "log_date_format": "DD-MM-YYYY hh:mm:ss a",
            "max_restarts": "1"
        }
    ]
}